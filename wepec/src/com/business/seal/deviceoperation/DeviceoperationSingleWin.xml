<?xml version="1.0" encoding="GBK"?>
 <page id="pageDeviceoperationSingleWin" mboname="pageDeviceoperationSingleWin"  beanclass="com.business.seal.deviceoperation.Deviceoperation" version="1.0">
   <window id="DeviceoperationSingleWin" title="选择设备操作" layout="fit" show="true" >
   	 <gridpanel id="DeviceoperationSingleWinList" menu=" " width="800"  height="480" tables="T_SEAL_DEVICEOPERATION"  singleSelect="true" op="block"  url="methodName=getListChoose"  autoload="true" closable="false">
