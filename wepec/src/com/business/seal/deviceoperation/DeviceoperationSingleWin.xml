        <script id="script"><![CDATA[
         function rvDeviceoperationSingle(p){
           var recordArray = p.param.getSelectionModel().getSelection();
           if(recordArray.length>0){
           eval(p.callback+"(recordArray)");
           }
           DeviceoperationSingleWin.close();
        }
       ]]>
       </script>
 </page>